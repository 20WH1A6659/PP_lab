arr = []
def linearsearch(arr,key,n):
    for i in range(n):
            if(arr[i] == key):
                  return i+1
    return -1
n = int(input("enter the no of array elements:")
   for ele in range(n):
      num = int(input("enter array elements:")
      arr.append(num)
        print(arr)
key = int(input("enter search element:")
ls = linearsearch(arr,key,n)
    if( ls == -1):
          print('{} is not found'.format(key))
    else:
        print('{} is found at {} position'.format(key,ls))
        
